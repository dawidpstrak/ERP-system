{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "start-test-server": "cd ../backend && NODE_ENV=test npm run start",
        "test": "jest --runInBand --detectOpenHandles"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "core-js": "^3.6.5",
        "dotenv": "^8.2.0",
        "http-status-codes": "^2.1.2",
        "moment": "^2.27.0",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.2",
        "vue": "^2.6.11",
        "vue-notification": "^1.3.20",
        "vue-router": "^3.3.4",
        "vuelidate": "^0.7.5",
        "vuetify": "^2.3.4",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.4.0",
        "@vue/cli-plugin-eslint": "~4.4.0",
        "@vue/cli-service": "~4.4.0",
        "babel-eslint": "^10.1.0",
        "chai": "^4.2.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.4.2",
        "puppeteer": "^5.3.0",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {
            "vue/attributes-order": [
                "error",
                {
                    "order": [
                        "DEFINITION",
                        "LIST_RENDERING",
                        "CONDITIONALS",
                        "RENDER_MODIFIERS",
                        "GLOBAL",
                        "UNIQUE",
                        "TWO_WAY_BINDING",
                        "OTHER_DIRECTIVES",
                        "OTHER_ATTR",
                        "EVENTS",
                        "CONTENT"
                    ],
                    "alphabetical": false
                }
            ]
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
